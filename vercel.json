{
  "version": 2,
  "buildCommand": "php -v",
  "framework": null,
  "functions": {
    "api/*.php": {
      "runtime": "@vercel/php@0.6.0"
    }
  },
  "outputDirectory": ".",
  "routes": [
    { "src": "/(.*)\\.php$", "dest": "/api/$1.php" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  }
}
